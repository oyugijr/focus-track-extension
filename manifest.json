{
    "manifest_version": 3,
    "name": "Focus Track",
    "version": "2.0",
    "description": "A Chrome extension for focus sessions with Pomodoro, task management, and ambient sounds.",
    "permissions": ["storage", "alarms", "notifications"],
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "options_page": "options.html"
  }
  